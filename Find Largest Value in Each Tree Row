var largestValues = function(root) {
    let res = [];
    let traverse = (root, level) => {
        if (root == null) return;

        if (res.length <= level) {
            res.push(root.val);
        } else {
            res[level] = Math.max(res[level], root.val);
        }
        traverse(root.left, level+1);
        traverse(root.right, level+1);
    }
    traverse(root, 0);
    return res;
};
