var subsets = function(nums) {
    const res = [[]];
    const stack = [[[], 0]];

    while (stack.length) {
        const [prev, start] = stack.pop();
        for (let i = start; i < nums.length; i++) {
            const newElem = [...prev, nums[i]];
            stack.push([newElem, i + 1]);
            res.push(newElem);
        }
    }
    return res;
};
